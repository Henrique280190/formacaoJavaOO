
public class TestaCaracteres {

	public static void main(String[] args) {
		
		char letra = 'a';
		
		System.out.println(letra);
		
		char valor = 65;
		System.out.println(valor);
		
		valor = (char) (valor +1);

		String palavra = "alura curso online tecnologia!";
		System.out.println(palavra);
	}

}
