//Gerente herda caracteristicas do funcionario
public class Gerente extends Funcionario {	
	
	private int senha = 2222;
	
	public boolean autentica(int senha){
		if(this.senha == senha){
			return true;
		}else{
			return false;
		}
	}
	
	//public double getBonificacao(){
